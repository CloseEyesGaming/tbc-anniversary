[32m+[m
[32m+[m[32m        elif action['type'] == 'gamepad' and HAS_GAMEPAD_LIB:[m
[32m+[m[32m            data = GAMEPAD_MAP.get(action['key'])[m
[32m+[m[32m            if data:[m
[32m+[m[32m                g_type = data[0][m
[32m+[m[32m                if g_type == 'btn':[m
[32m+[m[32m                    virtual_pad.press_button(button=data[1])[m
[32m+[m[32m                    virtual_pad.update()[m
[32m+[m[32m                    time.sleep(0.05)[m
[32m+[m[32m                    virtual_pad.release_button(button=data[1])[m
[32m+[m[32m                elif g_type == 'trigger':[m
[32m+[m[32m                    # data[1] is 'left' or 'right'[m
[32m+[m[32m                    if data[1] == 'left':[m
[32m+[m[32m                        virtual_pad.left_trigger(value=255)[m
[32m+[m[32m                    else:[m
[32m+[m[32m                        virtual_pad.right_trigger(value=255)[m
[32m+[m[32m                    virtual_pad.update()[m
[32m+[m[32m                    time.sleep(0.05)[m
[32m+[m[32m                    if data[1] == 'left':[m
[32m+[m[32m                        virtual_pad.left_trigger(value=0)[m
[32m+[m[32m                    else:[m
[32m+[m[32m                        virtual_pad.right_trigger(value=0)[m
[32m+[m[32m                elif g_type == 'stick':[m
[32m+[m[32m                    # data = ('stick', side, x, y)[m
[32m+[m[32m                    side, x, y = data[1], data[2], data[3][m
[32m+[m[32m                    if side == 'left':[m
[32m+[m[32m                        virtual_pad.left_joystick_float(x_value_float=x, y_value_float=y)[m
[32m+[m[32m                    else:[m
[32m+[m[32m                        virtual_pad.right_joystick_float(x_value_float=x, y_value_float=y)[m
[32m+[m[32m                    virtual_pad.update()[m
[32m+[m[32m                    time.sleep(0.05)[m
[32m+[m[32m                    if side == 'left':[m
[32m+[m[32m                        virtual_pad.left_joystick_float(x_value_float=0.0, y_value_float=0.0)[m
[32m+[m[32m                    else:[m
[32m+[m[32m                        virtual_pad.right_joystick_float(x_value_float=0.0, y_value_float=0.0)[m
[32m+[m
[32m+[m[32m                virtual_pad.update()[m
     except Exception as e:[m
