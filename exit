[32m+[m[32m        'MINUS': '-', 'EQUALS': '=', 'RETURN': 'enter', 'SPACE': 'space', 'TAB': 'tab',[m
[32m+[m[32m        'LEFTBRACKET': '[', 'RIGHTBRACKET': ']', 'BACKSLASH': '\\',[m
[32m+[m[32m        'SEMICOLON': ';', 'QUOTE': "'", 'COMMA': ',', 'PERIOD': '.', 'SLASH': '/'[m
[32m+[m[32m    }[m
     clean_key = lua_key.replace('NUMPAD', 'num ') if 'NUMPAD' in lua_key else key_map.get(lua_key, lua_key.lower())[m
     return {'type': 'keyboard', 'modifiers': modifiers, 'key': clean_key}[m
